{
  "name": "prisma-docker-basic",
  "version": "1.0.0",
  "description": "Proyecto base: Node.js/Express + Prisma ORM + PostgreSQL, ejecutable con Docker Compose.",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "node index.js",
    "start": "node index.js",
    "db:deploy": "npx prisma migrate deploy && npx prisma generate",
    "db:generate": "npx prisma generate",
    "db:status": "npx prisma migrate status",
    "studio": "npx prisma studio --port 5555 --browser none"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "^7.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@types/pg": "^8.11.10",
    "prisma": "^7.0.0"
  }
}
